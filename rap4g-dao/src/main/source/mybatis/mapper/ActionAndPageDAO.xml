<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- 目的：为DAO接口方法提供sql语句配置 -->
<mapper namespace="com.zufengze.rap4g.dao.ActionAndPageDAO">
	<insert id="insertActionAndPage" parameterType="com.zufengze.rap4g.dataobject.ActionAndPageDO"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO
		rap_db.tb_action_and_page (
		action_id,
		page_id
		)
		VALUES
		(
		#{actionId},
		#{pageId}
		)
	</insert>
</mapper>